{
  "version": 2,
  "env": {
    "LM_PROXY_BASE": "https://effects-stuff-pages-bikini.trycloudflare.com"
  },
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/**/*.ts", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/lm/(.*)", "destination": "/api/lm/$1" }
  ]
}


